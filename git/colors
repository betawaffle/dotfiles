[color]
  branch = true
  diff   = true
  status = true
  ui     = true

[color "branch"]
  current = magenta
  local   = blue
  remote  = yellow

[color "diff"]
  plain      = normal
  meta       = blue bold
  frag       = magenta bold
  func       = yellow
  old        = red
  new        = green
  commit     = blue
  whitespace = black white

[color "status"]
  added     = green
  changed   = yellow
  untracked = red
